{
  "name": "sq-reflect",
  "version": "1.0.0-0",
  "main": "index.js",
  "description": "sql typescript type generator",
  "repository": "git@github.com:harryparkdotio/sq-reflect.git",
  "author": "Harry Park <harry@harrypark.io>",
  "license": "MIT",
  "bin": {
    "sq-reflect": "./cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp package.json dist/ && cp readme.md dist/ && cp license dist/",
    "prettier": "prettier --write '{src,test}/**/*.ts' '*.{json,js,md}'",
    "cover": "codecov",
    "lint": "yarn lint:code && yarn lint:prettier",
    "lint:code": "tslint -t codeFrame '{src,test}/**/*.ts'",
    "lint:prettier": "prettier --list-different '{src,test}/**/*.ts' '*.{json,js,md}'",
    "test": "jest --runInBand",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:e2e": "jest test/e2e"
  },
  "dependencies": {
    "change-case": "^3.0.2",
    "common-tags": "^1.8.0",
    "dedent": "^0.7.0",
    "fs-extra": "^7.0.1",
    "pg": "^7.6.1",
    "tslib": "^1.9.3",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/common-tags": "^1.4.0",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^5.0.4",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.10",
    "@types/pg": "^7.4.11",
    "@types/yargs": "^12.0.1",
    "codecov": "^3.1.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint -t codeFrame",
      "prettier --write",
      "git add"
    ],
    "*.{json,js,md,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "yargs": {
    "boolean-negation": false
  }
}
