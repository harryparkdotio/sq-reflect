{
  "name": "sq-reflect",
  "version": "1.1.0",
  "main": "index.js",
  "description": "sql typescript type generator",
  "repository": "git@github.com:harryparkdotio/sq-reflect.git",
  "author": "Harry Park <harry@harrypark.io>",
  "license": "MIT",
  "bin": {
    "sq-reflect": "./cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "cp package.json dist/ && cp readme.md dist/ && cp license dist/",
    "prettier": "prettier --write '{src,test}/**/*.ts' '*.{json,js,md}'",
    "cover": "codecov",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest --runInBand",
    "test:unit": "jest test/unit",
    "test:integration": "jest test/integration",
    "test:e2e": "jest test/e2e"
  },
  "dependencies": {
    "change-case": "^3.1.0",
    "common-tags": "^1.8.0",
    "dedent": "^0.7.0",
    "fs-extra": "^8.0.1",
    "pg": "^7.11.0",
    "tslib": "^1.9.3",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/dedent": "^0.7.0",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.2",
    "@types/pg": "^7.4.14",
    "@types/yargs": "^13.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "codecov": "^3.5.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-import": "^2.17.2",
    "husky": "^2.3.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{json,md,yaml,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "yargs": {
    "boolean-negation": false
  }
}
